<% if @event.destroy %>
  $("#event-<%= @event_id %>").remove();
  $('#event-error').html("<%=j render 'partials/error_message', error_message: 'Event deleted successfully' %>");
<% else %>
  $('#event-error').html("<%=j render 'partials/error_message', error_message: 'Event deletion failed. Try again later' %>");
<% end %>
