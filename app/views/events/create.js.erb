<% if @event.save %>
    $("#event_event_text").val("");
    $("#feed").prepend("<%=j render @event %>");
    $('#event-error').html("<%=j render 'partials/error_message', error_message: 'Event added to schedule' %>");
<% else %>
    $('#event-error').html("<%=j render 'partials/error_message', error_message: @event.errors.full_messages.first %>");
<% end %>